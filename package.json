{
  "name": "backoffice-base-startups",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky install",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "ng serve --port 4444",
    "start:o": "ng serve --port 4444 -o",
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy:production": "ng deploy",
    "deploy:staging": "git checkout staging && git merge develop && yarn build:staging && yarn release && git push --follow-tags origin staging && git push && git checkout develop && git merge staging && git push",
    "deploy:main": "git checkout main && git merge staging && yarn build && yarn release && git push --follow-tags origin main && git push && git checkout staging && git merge main && git push && git checkout develop",
    "release": "standard-version",
    "standar-init": "standard-version --first-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.10",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/fire": "^17.1.0",
    "@angular/forms": "^17.3.10",
    "@angular/material": "17.3.10",
    "@angular/material-luxon-adapter": "17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/platform-server": "^17.3.10",
    "@angular/router": "^17.3.10",
    "@angular/ssr": "^17.2.1",
    "@ngneat/transloco": "6.0.4",
    "@ngrx/effects": "^17.1.1",
    "@ngrx/router-store": "^17.1.1",
    "@ngrx/store": "^17.1.1",
    "@ngrx/store-devtools": "^17.1.1",
    "apexcharts": "3.47.0",
    "crypto-js": "4.2.0",
    "express": "^4.18.2",
    "highlight.js": "11.9.0",
    "lodash-es": "4.17.21",
    "luxon": "3.4.4",
    "ng-apexcharts": "1.9.0",
    "ngx-quill": "25.1.1",
    "perfect-scrollbar": "1.5.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.1",
    "@angular/cli": "^17.2.1",
    "@angular/compiler-cli": "^17.3.10",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@tailwindcss/typography": "0.5.10",
    "@types/chroma-js": "2.4.4",
    "@types/crypto-js": "4.2.2",
    "@types/express": "^4.17.17",
    "@types/highlight.js": "10.1.0",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "4.14.202",
    "@types/lodash-es": "4.17.12",
    "@types/luxon": "3.4.2",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "10.4.18",
    "chroma-js": "2.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lodash": "4.17.21",
    "postcss": "8.4.35",
    "standard-version": "^9.5.0",
    "tailwindcss": "3.4.1",
    "typescript": "~5.3.2"
  }
}
